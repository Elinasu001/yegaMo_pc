    function check_incom_annual(success, fail) {
		// 연소득
		var incom_annual = $('#incom_annual').val();
		if(incom_annual != '' && incom_annual.length >= 6) {
			removeLoading();
			var message = '연소득 금액을 만원 단위로 입력해주세요.'
    		customAlert({title : '안내', message : message}, function() {
    			focus_to('#incom_annual_format');
    			fail();
    		});
    	}else {
    		success();
    	}
	}
	
