    function req_next(){
    	var creditChkCnt = 0;
    	var pubChkCnt = 0;
    	var chkProd ='';
    	$('input:checkbox[name="credit_check"]').each(function(){
    		if(this.checked){
    			creditChkCnt++;
    			chkProd += this.value +'|';
    		}
    	});
    	
    	$('input:checkbox[name="pub_check"]').each(function(){
    		if(this.checked){
    			pubChkCnt++;
    			chkProd += this.value +'|';
    		}
    	});
    	
    	if(creditChkCnt>1){
    		customAlert({title : "알림" , message : "신용상품은 하나만 선택해 주세요." }, function() {
    		});
    		return;
    	}
    	
    	if(creditChkCnt==0 && pubChkCnt==0){
    		customAlert({title : "알림" , message : "최소 하나의 상품을 선택해 주세요." }, function() {
    		});
    		return;
    	
    	}
    	
    	chkProd = chkProd.slice(0,-1);
    	$("#PROD_CD").val(chkProd);
    	reqFlag = true;
    	loading();
		AjaxFunc.ajaxAsync('<c:url value="${loan_request_proc}" />', $('#req_frm').serialize(), function(res) {
			removeLoading();
			if(res.result) {
				if(res.resultCode=='0'){
					if(res.data.RES_CODE == 1000){
						location.href = '<c:url value="${complete}"/>';	
					}else{
						customAlert({title : "알림" , message : res.data.RES_MESSAGE }, function() {});
					}
				}else{
					customAlert({title : '알림', message : res.resultMessage },function(){
					});
				}
				
			}else  {
				on_error(res);
			}
		}, on_error);
    	
    }
    
