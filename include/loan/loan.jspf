	<%-- 입사일 설정시, 재직기간 자동 계산. --%>
	function job_join_day_update() {
		if($('#job_period').is(':visible')) {
    		var value = this.value;
    		var job_period = '0';
    		if(value.length == 8 && new Date([value.substr(0, 4), value.substr(4, 2), value.substr(6)].join('-')) != 'Invalid Date') {
    			var current = new Date();
    			var input = new Date([value.substr(0, 4), value.substr(4, 2), value.substr(6)].join('-'));
    			var months = UtilFunc.getDateDiffInMonths(input, current);
    			job_period = months < 0 ? 0 : months;
    		}
    		$('#job_period').val(job_period);
		}
	}
	
    $(function() {
    	$('#job_join_day').on('change', job_join_day_update);
    	$('#job_join_day').on('keyup', job_join_day_update);
    })
    
