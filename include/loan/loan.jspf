    <%-- 공공 마이데이터 서류 등록. --%>
    function ajaxMydataList(job_cd) {
    	var param ={"JOB_CD":job_cd};
    	
    	if(job_cd == 'C002000003' || job_cd == 'C002000008') {
    		$('.bnum').show();
    	}else{
    		$('.bnum').hide();
    	}
    	
		AjaxFunc.ajaxAsync('<c:url value="${ajaxMydataList}" />', param, function(res){
			if(res.result) {
				var str = "";
				
				mydataScrapCodeList = res.data;
				mydataScrapTypeItemMap[job_cd].scrapCdList.forEach(function(item){
					mydataScrapCodeList.forEach(function(scrapCode){
						if(item == scrapCode.CODE) str += '<li> • ' + scrapCode.NAME + '</li>';
					});
				});

				$('#mydataScrapSelectList').html(str);
				$('#mydataScrapSelectList').parent().show();
				
		
				if('C002000003' == job_cd || 'C002000008' == job_cd){
					$('#viewBnum').show();
					bizNoTarget = "Y";
				}else{
					$('#viewBnum').hide();
					bizNoTarget = "N";
				}
				
				mydataParam.jobType = job_cd;
				mydataParam.scrapCdList = mydataScrapTypeItemMap[job_cd].scrapCdList;
				
			}else {
				on_error(res);
			}
		}, on_error);
    }
    
    <%-- 오류응답에 대한 공통처리 --%>
    function on_error(err) {
    	if(!err) err = {};
    	if(!err.message) {
    		err.message = err.resultMessage;
    		if(!err.message) err.message = "서비스중 장애발생.";
    	}
    	removeLoading();
		customAlert({title : '안내', message: err.message}, function() {
			if(!!err && !!err.message && err.message.indexOf('세션') > -1) { // 세션만료
				
				location.href = '<c:url value="${home}" />'
			}
			if(!!err && !!err.message && err.message.indexOf('월 접수건') > -1) {
				
				location.href = '<c:url value="${home}" />'
			}
		});
    }
    
