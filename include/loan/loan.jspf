	/**
	 * 사용자 입력항목 보여줌 처리
	 */
	function form_input_show(id) {
		var $item = $('#' + id);
		if(id!='mrgg_tp_cd'){
			$item.val('');
		}
		$item.parents('.grid2').addClass('mb30');
		$item.parents('.form-wrap').fadeIn();
	}
	
	/**
	 * 사용자 입력항목 숨김 처리
	 */	
	function form_input_hide(id) {
		var $item = $('#' + id);
		if(id!='mrgg_tp_cd'){
			$item.val('');
		}
		$item.parents('.form-wrap').fadeOut();
		$item.parents('.grid2').removeClass('mb30');
	}
    
