			customAlert({title : '안내', message : "기업대출상품설명서는 필수 약관입니다."});
			return false;
		}
		return true;
	}
	
	function myDataStart() {
		//사업자등록번호 입력 대상
		if("Y" == bizNoTarget) mydataParam.mydataBnum = $('#bnum1').val() + $('#bnum2').val() + $('#bnum3').val();
		
		//법인사업자인 경우 개인사업자 코드로 처리하기로 정함
// 		if("C002000008" == mydataParam.jobType) mydataParam.jobType = "C002000003";
		
		var param = {"mydataParam":JSON.stringify(mydataParam)};
		
		if(JSON.stringify(param) == "{}"){
			customAlert({title:"확인", message:"직업구분을 선택해주세요.", btn_txt1:"닫기"}, '', function(){ return; });
			return;
		}
//		location.href = '<c:url value="${complete}"/>';	
		$('.authArea').hide();
		$('.mydataArea').show();
		
		
		ajaxSrchMydata(param);
// 		ajaxSrchMydataAgree(param);
	}
	
	function ajaxSrchMydataAgree(param) {
		AjaxFunc.ajaxAsync('<c:url value="${ajaxMydataAgree}" />', param, function(res) {
			if(res.result) {
// 				ajaxSrchMydata(param);
				request_loan(param);
				closePopup('authLayer');
				loading();				
			}else {
				on_error(res);
				closePopup('authLayer');
				var msg = res.resultMsg == "" ? '공공마이데이터 조회 중 오류가 발생하였습니다.\n다시 시도해주세요.' : res.resultMsg;
				customAlert({title : '안내', message : msg}, function() {
					return;
	    		});
			}
		}, function(res) {
			on_error(res);
			closePopup('authLayer');
		});
	}
	
	function ajaxSrchMydata(param) {
		AjaxFunc.ajaxAsync('<c:url value="${ajaxMydataResult}" />', param, function(res) {
			if(res.result) {
// 				request_loan();
// 				setLimitList(res); //20240404
				$('#INSP_NO').val(res.data.INSP_NO);
				ajaxSrchMydataAgree(res.data);
		    	return;
			}else {
				on_error(res);
				closePopup('authLayer');
				var msg = res.resultMsg == "" ? '공공마이데이터 조회 중 오류가 발생하였습니다.\n다시 시도해주세요.' : res.resultMsg;
				customAlert({title : '안내', message : msg}, function() {
					return;
	    		});
			}
		}, function(res) {
			on_error(res);
			closePopup('authLayer');
			var msg = res.resultMsg == "" ? '공공마이데이터 조회 중 오류가 발생하였습니다.\n다시 시도해주세요.' : res.resultMsg;
			customAlert({title : '안내', message : msg}, function() {
				return;
    		});
		});
	}
	
	
	</script>