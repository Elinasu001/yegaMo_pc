					}else if(res.data.RES_CODE == 5001){
							location.href = '<c:url value="${limitFail}"/>';	
					}else{
						customAlert({title : "알림" , message : res.data.RES_MESSAGE }, function() {});
					}
				}else{
					customAlert({title : '알림', message : res.resultMessage },function(){
					});
				}
				
			}else  {
				on_error(res);
			}
		}, on_error);
    }
    
    
