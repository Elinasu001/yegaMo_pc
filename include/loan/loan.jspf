	function getTermsCode(){
		var terms = config.terms[$("#prod_cd").val()];
		var termsCode = [];

		if(terms != '' && terms != null){
			for(var i = 0; i < terms.show.length; i++){
				var code = $('#' + terms.show[i]).parent().parent().attr('id');
				termsCode.push(code);	
			}
		}
		return termsCode;
	}
	
